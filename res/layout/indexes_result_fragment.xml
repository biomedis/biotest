<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:orientation="horizontal"
              android:layout_width="match_parent"
              android:layout_height="match_parent" android:baselineAligned="false"
        android:minHeight="150dp"
              android:gravity="center_horizontal"
        >




    <LinearLayout
                  android:orientation="vertical"
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:layout_weight="1"
            android:gravity="center_horizontal">
            <view
                    android:id="@+id/indexBar1"
                    android:height="1dp"
                    android:width="45dp"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    class="ru.biomedis.biotest.graph.SingleBarGraphImpl.IntegerSingleBarGraph"


                    android:background="#FFFFFF"
                    android:layout_weight="10"/>
        <LinearLayout     android:orientation="horizontal"    android:layout_width="wrap_content"   android:layout_height="wrap_content"  >
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/IN_abbr"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/indexIN"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal">
        <view
                android:id="@+id/indexBar2"
                android:height="1dp"
                android:width="45dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                class="ru.biomedis.biotest.graph.SingleBarGraphImpl.IntegerSingleBarGraph"

                android:layout_weight="10"
                android:background="#FFFFFF"
                />
        <LinearLayout     android:orientation="horizontal"    android:layout_width="wrap_content"   android:layout_height="wrap_content"  >
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/IS_abbr"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/indexIS"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal">
        <view
                android:id="@+id/indexBar3"
                android:height="1dp"
                android:width="45dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                class="ru.biomedis.biotest.graph.SingleBarGraphImpl.IntegerSingleBarGraph"

                android:layout_weight="10"
                android:background="#FFFFFF"
                />
        <LinearLayout     android:orientation="horizontal"    android:layout_width="wrap_content"   android:layout_height="wrap_content"  >
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/BAK_abbr"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/indexBAK"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center_horizontal">
        <view
                android:id="@+id/indexBar4"
                android:height="1dp"
                android:width="45dp"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                class="ru.biomedis.biotest.graph.SingleBarGraphImpl.IntegerSingleBarGraph"

                android:layout_weight="10"
                android:background="#FFFFFF"
                />
        <LinearLayout     android:orientation="horizontal"    android:layout_width="wrap_content"   android:layout_height="wrap_content" >
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/BE_abbr"/>
            <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/indexBE"/>
        </LinearLayout>
    </LinearLayout>


</LinearLayout>