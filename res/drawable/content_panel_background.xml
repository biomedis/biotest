<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle" >
            <solid android:color="@color/content_panel_color"></solid>

        </shape>
    </item>

    <item android:bottom="6dp" android:top="6dp" android:left="6dp" android:right="6dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"></solid>
            <stroke android:color="#35cdcecf" android:dashWidth="1dp" android:width="1dp" android:dashGap="3dp"></stroke>
        </shape>
    </item>
    <item android:bottom="6dp" android:top="6dp" android:left="6dp" android:right="6dp">
        <bitmap android:src="@drawable/setka1" android:tileMode="repeat"></bitmap>
            </item>


</layer-list>